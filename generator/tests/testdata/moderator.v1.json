      },
      "hasAttachedVideo": {
       "location": "query",
       "description": "Specifies whether to restrict to submissions that have videos attached.",
       "type": "boolean"
      },
      "max-results": {
       "location": "query",
       "description": "Maximum number of results to return.",
       "type": "integer",
       "minimum": "0",
       "maximum": "4294967295"
      },
      "q": {
       "location": "query",
       "description": "Search query.",
       "type": "string"
